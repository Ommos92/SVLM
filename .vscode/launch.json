{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "SEEM API Server",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "train",
                "--conf_files", "SEEM/configs/xdecoder/focall_unicl_lang.yaml",
                "--user_dir", "results",
            ],
            "env": {"CUDA_VISIBLE_DEVICES": "3"}
        },
        {
            "name": "Launch Stable Diffusion API Server",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": ["Stable-Diffusion.api.model_server:app", "--host", "0.0.0.0", "--port", "8000"],
            "jinja": true,
            "env": {
                "PYTHONPATH": "~/miniconda3/envs/xformers_env/bin/python",
                "CUDA_VISIBLE_DEVICES": "3"
            }
        }
    ]
        
    
}